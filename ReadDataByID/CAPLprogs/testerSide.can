/*@!Encoding:65001*/
includes
{
  
}

variables
{
  msTimer mytime;
  message 0x300 tester;
}
on start
{
  setTimer(mytime,1000);
  write("Tester started");
}
on message 0x300
{
  tester.dlc=4;
}

