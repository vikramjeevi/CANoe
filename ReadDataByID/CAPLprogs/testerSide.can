on timer mytime
{
  setTimer(mytime,1000);
  tester.byte(0)=@sysvar::dd_var::byte0;
  tester.byte(1)=@sysvar::dd_var::byte1;
  tester.byte(2)=@sysvar::dd_var::byte2;
  tester.byte(3)=@sysvar::dd_var::byte3;
  output(tester);
  
}
on key 's'
{
  cancelTimer(mytime); 
}